{
  "extends": [
    "config:base"
  ],
  "enabledManagers": ["cargo","dockerfile","github-actions"],
  "packageRules": [
    {
      "matchUpdateTypes": ["major", "minor", "patch", "pin", "digest"],
      "automerge": false
    }
  ],
  "vulnerabilityAlerts": {
    "labels": ["security"],
    "automerge": false
  },
  "lockFileMaintenance": {
    "enabled": true
  },
  "prHourlyLimit": 0
}
